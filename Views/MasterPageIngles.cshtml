        if (@Perfil == 2) {
            document.getElementById('CRM').style.display = 'none';
            document.getElementById('ALAB').style.display = 'none';
            document.getElementById('Planificacion').style.display = 'none';
        }else if(@Perfil == 3){
            document.getElementById('Tecnica').style.display = 'none';
            document.getElementById('CRM').style.display = 'none';
            document.getElementById('ALAB').style.display = 'none';
            document.getElementById('Planificacion').style.display = 'none';
        }else if(@Perfil == 4){
            document.getElementById('Servicios').style.display = 'none';
            document.getElementById('Tecnica').style.display = 'none';
            document.getElementById('CRM').style.display = 'none';
            document.getElementById('ALAB').style.display = 'none';
            document.getElementById('Planificacion').style.display = 'none';
        }else if(@Perfil == 5){
            document.getElementById('DMS').style.display = 'none';
            document.getElementById('Tecnica').style.display = 'none';
            document.getElementById('CRM').style.display = 'none';
            document.getElementById('ALAB').style.display = 'none';
            document.getElementById('ModificarDatos').style.display = 'none';
            document.getElementById('Planificacion').style.display = 'none';
        }else if(@Perfil == 6){
            document.getElementById('CRM').style.display = 'none';
            document.getElementById('Planificacion').style.display = 'none';
        }else if(@Perfil == 7){
            document.getElementById('CRM').style.display = 'none';
        }else if(@Perfil == 8){
            document.getElementById('ALAB').style.display = 'none';
        }

        $(window).load(function(){
            $('#dvLoading').fadeOut(3000);
        });
